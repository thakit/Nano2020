@import url('https://fonts.googleapis.com/css2?family=Roboto:wght@400;500;700&display=swap');
@import url('https://fonts.googleapis.com/css2?family=Sarabun:wght@400;500;600&display=swap');
@import url('https://fonts.googleapis.com/css2?family=Bebas+Neue&display=swap');
* {
    margin: 0;
    padding: 0;
    box-sizing: border-box;
    font-family: 'Roboto', sans-serif;
}
nav#navigation-bar {
    max-width: 100%;
    height: 160px;
}
.top-nav-wrapper {
    width: 100%;
    height: 30px;
    background: #00ae41;
    overflow: hidden;
    position: relative;
    z-index: 9999;
}
.container {
    max-width: 980px;
    margin: 0 auto;
}
.top-nav {
    width: 100%;
    height: 30px;
    display: flex;
    align-items: center;
    justify-content: flex-end;
    color: #fff;
    font-size: .8rem;
}
.nav-language {
    width: 65px;
    margin-left: 10px;
    background-color: #013f16;
    padding: 8px 12px;
    display: flex;
    justify-content: space-between;
    align-items: center;
}
.btn-select-language i {
    font-size: 1rem;
    cursor: pointer;
}
.bottom-nav-wrapper {
    width: 100%;
    height: 130px;
    overflow: hidden;
    background: rgba(255, 255, 255, .5);
}
.nav-logo {
    margin: 1rem 0;
    display: flex;
    justify-content: space-between;
    align-items: center;
}
.btn-offset {
    display: none;
    cursor: pointer;
    background-color: #095845;
    padding: 5px 12px;
    border-radius: 4px;
}
.bar1, .bar2, .bar3 {
    width: 35px;
    height: 5px;
    background-color: #fff;
    margin: 6px 0;
    transition: 0.4s;
}

.nav-wrapper {
    display: flex;
    justify-content: space-between;
}
ul.list-nav-menu {
    display: flex;
    width: 740px;
    justify-content: space-between;
    list-style: none;
}
ul.list-nav-menu li a {
    text-decoration: none;
    color: #00ae41;
    font-weight: 700;
    font-size: .9rem;
    transition: .3s;
}
ul.list-nav-menu li a:hover {
    color: #095845;
}
ul.list-nav-social {
    display: flex;
    width: 150px;
    justify-content: space-between;
}
ul.list-nav-social img {
    width: 1.2rem;
    cursor: pointer;
}

@media only screen and (max-width: 980px) {
    .container {
        max-width: 720px;
        position: relative;
    }
    nav#navigation-bar {
        height: 120px;
        position: sticky;
        top: 0;
        z-index: 99999;
    }
    .bottom-nav-wrapper {
        height: 90px;
    }
    .btn-offset {
        display: inline-block;
        position: absolute;
        right: 0;
        transition: right .5s;
    }
    .show.btn-offset {
        right: 220px;
    }
    .show .bar1 {
        -webkit-transform: rotate(-45deg) translate(-9px, 6px);
        transform: rotate(-45deg) translate(-9px, 6px);
    }
    .show .bar2 {
        opacity: 0;
    }
    .show .bar3 {
        -webkit-transform: rotate(45deg) translate(-8px, -8px);
        transform: rotate(45deg) translate(-8px, -8px);
    }
    .nav-wrapper {
        position: fixed;
        width: 220px;
        height: 100%;
        right: -230px;
        top: 0;
        bottom: 0;
        background: #00ae41;
        box-shadow: -4px 0 15px rgba(0, 0, 0, .3);
        flex-direction: column;
        justify-content: start;
        align-items: center;
        transition: right .5s;
        z-index: 999;
    }
    .show.nav-wrapper {
        right: 0;
    }
    ul.list-nav-menu {
        padding-top: 2rem;
        margin: 2rem 0;
        width: 100%;
        display: inline-block;
        line-height: 4;
        text-align: center;
    }
    ul.list-nav-menu li a {
        transition: .3s;
        display: block;
        color: #013f16;
    }
    ul.list-nav-menu li:hover a{
        background: #095845;
        cursor: pointer;
        color: #fff;
    }
}
@media only screen and (max-width: 740px) {
    .container {
        max-width: 580px;
    }
}
@media only screen and (max-width: 600px) {
    .container {
        max-width: 480px;
    }
}
@media only screen and (max-width: 500px) {
    .container {
        width: calc(100% - 40px);
    }
}

section#banner img{
    width: 100%;
}


