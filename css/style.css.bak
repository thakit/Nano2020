}

@media only screen and (max-width: 850px) {
    .company-info p {
        font-size: .7rem;
    }
    .company-info p:nth-child(1) {
        padding-left: 30px;
    }
    .company-info p:nth-child(2) {
        padding-left: 24px;
    }
    .company-info p:nth-child(3) {
        padding-left: 18px;
    }
    .company-info p:nth-child(4) {
