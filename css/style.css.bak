@media only screen and (max-width: 660px) {
    .company-info p {
        font-size: .5rem;
    }
    .company-info p:nth-child(1) {
        padding-left: 20px;
    }
    .company-info p:nth-child(2) {
        padding-left: 16px;
    }
    .company-info p:nth-child(3) {
        padding-left: 12px;
    }
    .company-info p:nth-child(4) {
        padding-left: 8px;
    }
    .company-info p:nth-child(5) {
        padding-left: 4px;
    }
    .btn-read a {
        font-size: .9rem;
    }
}
@media only screen and (max-width: 470px) {
    .company-info p {
        font-size: .4rem;
    }
    .btn-read a {
        font-size: .7rem;
    }
    .company-info p:nth-child(1) {
        padding-left: 15px;
    }
    .company-info p:nth-child(2) {
        padding-left: 12px;
    }
    .company-info p:nth-child(3) {
        padding-left: 9px;
    }
    .company-info p:nth-child(4) {
        padding-left: 6px;
    }
    .company-info p:nth-child(5) {
        padding-left: 3px;
    }
}

#product-highlight {
    padding: 3rem 0;
}
.product-highlight-topic {
    display: flex;
    justify-content: center;
    align-items: center;
}
.line-topic {
    width: 20%;
    height: 3px;
    background: #00ae41;
}
.product-highlight-topic h1 {
    font-size: 2.2rem;
    color: #00ae41;
    padding: 0 1rem;
    letter-spacing: 2px;
}
.product-highlight-products-wrapper {
    width: 100%;
    display: grid;
    grid-template-columns: 260px 260px 1fr;
    grid-template-rows: 50% 50%;
    grid-gap: 1.2rem;
    margin: 3rem 0;
}
.product-highlight-products-items-1 {
    grid-column: 1/2;
    grid-row: 1/2;
    box-shadow: 0 0 10px rgba(0, 0, 0, .4);
    border-radius: 6px;
    padding: .8rem 1.5rem;
    display: grid;
    align-content: space-between;
}
.product-highlight-products-items-2 {
    grid-column: 1/2;
    grid-row: 2/3;
    box-shadow: 0 0 10px rgba(0, 0, 0, .4);
    border-radius: 6px;
    padding: .8rem 1.5rem;
    display: grid;
    align-content: space-between;
}
.product-highlight-products-items-3 {
    grid-column: 2/3;
    grid-row: 1/2;
    box-shadow: 0 0 10px rgba(0, 0, 0, .4);
    border-radius: 6px;
    padding: .8rem 1.5rem;
    display: grid;
    align-content: space-between;
}
.product-highlight-products-items-4 {
    grid-column: 2/3;
    grid-row: 2/3;
    box-shadow: 0 0 10px rgba(0, 0, 0, .4);
    border-radius: 6px;
    padding: .8rem 1.5rem;
    display: grid;
    align-content: space-between;
}
.product-highlight-products-items-5 {
    grid-column: 3/5;
    grid-row: 1/3;
    box-shadow: 0 0 10px rgba(0, 0, 0, .4);
    border-radius: 6px;
    padding: .8rem 1.5rem;
    display: grid;
    justify-content: space-evenly;
    align-items: center;
    text-align: center;
}
.product-highlight-products-items-img {
    text-align: center;
}
.product-highlight-products-items-1 .product-highlight-products-items-img img{
    width: 90%;
}
.product-highlight-products-items-2 .product-highlight-products-items-img img{
    width: 60%;
}
.product-highlight-products-items-img img {
    width: 50%;
}
.product-highlight-products-items-description p {
    font-size: .9rem;
    font-family: 'Sarabun', sans-serif;
}
.product-highlight-products-items-promotion {
    display: flex;
    justify-content: space-between;
    align-items: center;

}
.product-highlight-products-items-promotion span img {
    width: 25px;
}
.product-highlight-products-items-promotion span:nth-child(1) {
    background-color: #ff3d3d;
    color: #fff;
    padding: 5px;
    font-weight: 700;
}
.product-highlight-products-items-5 .product-highlight-products-items-img img {
    width: 100%;
}
.product-highlight-products-items-5 .product-highlight-products-items-description {
    text-align: left;
}
.product-highlight-products-items-5 .product-highlight-products-items-promotion {
    display: block;
}
.product-highlight-products-items-5 .product-highlight-products-items-promotion span{
    font-size: 1.4rem;
}
.product-highlight-products-items-5 span img{
    width: 35px;
}

.product-highlight-catalog-wrapper {
    display: grid;
    grid-template-columns: 50% 50%;
    padding: 1rem 0;
}
.product-highlight-catalog-download {
    background: #00ae41;
    padding: 2rem;
    display: flex;
    align-items: center;
    justify-content: space-between;
}
.catalog-download-title h2 {
    font-size: 1.6rem;
    color: #fff;
    letter-spacing: 1.5px;
    font-family: 'Bebas Neue', cursive;
    padding: .6rem 0;
}
.btn-click-catalog-download button{
    width: 100%;
    color: #013f16;
    border: none;
    padding: 2px 0;
    font-weight: 700;
    font-size: 1.2rem;
    border-radius: 50px;
}
.catalog-download-select-pdf, .catalog-download-select-ebook {
    width: 120px;
    display: flex;
    justify-content: space-between;
    background: #095845;
    color: #fff;
    padding: 5px 10px;
    margin: 1rem 0;
}
.product-highligh-shop-new-click {
    background: #095845;
    padding: 2rem;
    display: flex;
    align-items: center;
    justify-content: space-between;
}
.shop-new-click-img {
    width: 18%;
}
.shop-new-click-img img{
    width: 100%;
}
.icon-cart-plus{
    width: 25%;
}
.icon-cart-plus img{
    width: 100%;
}

.title-info h3{
    color: #fff;
    font-size: .9rem;
    text-align: right;
    font-family: Arial, Helvetica, sans-serif;
    padding: .6rem 0;
}
.title-info p {
    font-size: .9rem;
    font-family: 'Sarabun', sans-serif;
    text-align: right;
    color: #fff;
}
.title-info p:last-child{
    font-size: .7rem;
    padding: .6rem 0;
}

.product-highlight-ensure-wrapper {
    margin: 1rem 0;
}
.product-highlight-ensure-wrapper ul {
    display: flex;
    justify-content: space-between;
    align-items: center;
}
.product-highlight-ensure-wrapper ul li {
    list-style: none;
    text-align: center;
}
.product-highlight-ensure-wrapper ul li img {
    width: 80%;
    
}

@media only screen and (max-width: 980px) {
    .product-highlight-products-wrapper {
        grid-template-columns: 200px 200px 1fr;
    }
    .product-highlight-catalog-wrapper {
        grid-template-columns: 100%;
    }
    .shop-new-click-img {
        width: 13%;
    }
    .icon-cart-plus {
        width: 18%;
    }
}
@media only screen and (max-width: 740px) {
    .product-highlight-topic h1 {
        font-size: 1.7rem;
    }
    .product-highlight-products-wrapper {
        grid-template-columns: 150px 150px 1fr;
    }
    .product-highlight-products-items-description p{
