    }
}
@media only screen and (max-width: 600px) {
    .product-highlight-topic h1 {
        font-size: 1.3rem;
    }
    .product-highlight-products-wrapper {
        grid-template-columns: 130px 130px 1fr;
    }
    .product-highlight-products-items-5 {
        padding: .6rem .8rem;
    }
    .product-highlight-products-items-promotion {
        font-size: .7rem;
    }
    .product-highlight-products-items-description p{
        font-size: .6rem;
    }
    .product-highlight-products-items-promotion span img {
        width: 20px;
    }
    .product-highlight-ensure-wrapper {
        margin: .5rem 0;
    }
}
@media only screen and (max-width: 500px) {
    .product-highlight-topic h1 {
        font-size: 1rem;
        padding: 0 .4rem;
    }
    .line-topic {
        width: 15%;
    }
    .product-highlight-products-wrapper {
        grid-template-columns: 100px 100px 1fr;
    }
    .product-highlight-products-items-1, .product-highlight-products-items-2,
    .product-highlight-products-items-3, .product-highlight-products-items-4,
    .product-highlight-products-items-5 {
        padding: .5rem;
    }
    .product-highlight-products-items-promotion {
        font-size: .5rem;
    }
    .product-highlight-products-items-description p{
        font-size: .5rem;
    }
    .product-highlight-products-items-promotion span img {
        width: 15px;
    }
    .product-highlight-products-items-5 .product-highlight-products-items-promotion span{
        font-size: .8rem;
    }
    .catalog-download-img {
        width: 18%;
    }
    .catalog-download-img img{
        width: 100%;
    }
    .catalog-download-title h2 {
        font-size: 1.2rem;
    }
    .catalog-download-select-pdf, .catalog-download-select-ebook {
        width: 100px;
    }
    .title-info h3{
        font-size: .7rem;
        padding: .3rem 0;
    }
    .title-info p {
        font-size: .6rem;
        font-family: 'Sarabun', sans-serif;
        text-align: right;
        color: #fff;
    }
    .title-info p:last-child{
        font-size: .4rem;
        padding: .3rem 0;
    }
}

section#nano-product-wrapper {
    max-width: 100%;
    height: 100%;
    background: url('../img/bg-product.png');
    overflow: hidden;
    padding: 4rem 0;
}
.nano-products-topic-wrapper {
    display: grid;
    justify-content: center;
}
.nano-products-topic {
    transform: skew(-20deg);
    background-color: #fff;
    width: 380px;
    text-align: center;
}
.nano-products-topic h1 {
    transform: skew(20deg);
    padding: 10px 2rem;
    color: #00ae41;
    font-size: 2.2rem;
    letter-spacing: 2px;
}
.nano-products-total-wrapper {
    display: grid;
    grid-template-columns: repeat(auto-fit,minmax(300px,1fr));
    justify-items: center;
    margin-top: 2rem;
}
.product-items {
    margin: .8rem;
    background-color: #fff;
    width: 300px;
    box-shadow: 0 0 15px rgba(0, 0, 0, 0.5);
}
.img-of-product {
    width: 100%;
    height: 185px;
    position: relative;
}
.img-of-product img {
    width: 100%;
}
.name-of-product {
    position: absolute;
    bottom: 0;
    left: 50%;
    transform: translateX(-50%);
    width: 90%;
    text-align: center;
}
.name-of-product h4 {
    background: #00ae41;
    padding: .6rem 0;
    color: #fff;
    border-radius: 5px;
}
.description-of-product {
    padding: 1rem;
    line-height: 1.5;
}
.description-of-product li {
    list-style: none;
    font-size: .9rem;
    font-family: 'Sarabun', sans-serif;
}
@media only screen and (max-width: 500px) {
    .nano-products-topic h1 {
        font-size: 1.2rem;
    }
    .nano-products-topic {
        width: 300px;
    }
}

section#solution-wrapper {
    background: #e5f0ea;
    padding: 4rem 0;
}
.solution-topic {
    text-align: center;
    padding-bottom: 2rem;
}
.solution-topic h1 {
    font-size: 2.2rem;
    color: #00ae41;
}
.solution-category-total {
    display: grid;
    grid-template-columns: 310px 310px 310px;
    grid-template-rows: 220px 220px;
    justify-content: center;
    grid-gap: 1.5rem;
}
.solution-items {
    box-shadow: 0 0 15px rgba(0, 0, 0, .4);
    transition: .5s;
    cursor: pointer;
    position: relative;
}
.solution-items:hover {
    transform: translateY(-5px);
}
.items-title {
    position: absolute;
    bottom: 0;
    right: 0;
    background-color: rgba(0, 174, 65, .5);
    width: 100%;
    text-align: right;
    padding: .8rem;
}
.solution-items:nth-child(1) .items-title {
    text-align: center;
}
.items-title h3 {
    color: #fff;
    text-shadow: 2px 2px 5px rgba(0, 0, 0, .8);
    font-family: 'Sarabun', sans-serif;
}
.solution-items:nth-child(1) {
    grid-column: 1/3;
    grid-row: 1/2;
    background: url('../img/solution1.png') no-repeat;
    background-size: cover;
    background-position: center;
}
.solution-items:nth-child(2) {
    grid-column: 1/2;
    grid-row: 2/3;
    background: url('../img/solution3.png') no-repeat;
    background-size: cover;
    background-position: center;
}
.solution-items:nth-child(3) {
    grid-column: 2/3;
    grid-row: 2/3;
    background: url('../img/solution4.png') no-repeat;
    background-size: cover;
    background-position: center;
}
.solution-items:nth-child(4) {
    grid-column: 3/4;
    grid-row: 1/2;
    background: url('../img/solution2.png') no-repeat;
    background-size: cover;
    background-position: center;
}
.solution-items:nth-child(5) {
    grid-column: 3/4;
    grid-row: 2/3;
    background: url('../img/solution5.png') no-repeat;
    background-size: cover;
    background-position: center;
}

@media only screen and (max-width: 980px){
    .solution-category-total {
        grid-template-columns: 350px 350px;
        grid-template-rows: 220px 220px 220px;
    }
    .solution-items:nth-child(4) {
        grid-column: 1/2;
        grid-row: 3/4;
        background: url('../img/solution2.png') no-repeat;
        background-size: cover;
        background-position: center;
    }
    .solution-items:nth-child(5) {
        grid-column: 2/3;
        grid-row: 3/4;
        background: url('../img/solution5.png') no-repeat;
        background-size: cover;
        background-position: center;
    }
}
@media only screen and (max-width: 740px) {
    .solution-category-total {
        grid-template-columns: 280px 280px;
        grid-template-rows: 200px 200px 200px;
    }
}
@media only screen and (max-width: 600px) {
    .solution-category-total {
        grid-template-columns: 230px 230px;
        grid-template-rows: 160px 160px 160px;
    }
}
@media only screen and (max-width: 500px) {
    .solution-category-total {
        grid-template-columns: 180px 180px;
        grid-template-rows: 120px 120px 120px;
    }
    .items-title {
        padding: .5rem;
    }
    .items-title h3 {
        font-size: .8rem;
    }
    .solution-topic h1 {
        font-size: 1.8rem;
    }
}

section#news-event-wrapper {
    width: 100%;
    padding: 4rem 0;
}
.news-event-topic {
    text-align: center;
    padding-bottom: 1rem;
}
.news-event-topic h1 {
    font-size: 2.2rem;
    color: #00ae41;
    letter-spacing: 2px;
}
.news-event-total-wrapper {
    display: grid;
    grid-template-columns: repeat(auto-fit, minmax(320px, 1fr));
    justify-items: center;
}
.news-event-items {
    margin: 1rem;
}
.news-event-img {
    width: 100%;
}
.news-event-img img {
    width: 100%;
}
.news-event-description {
    padding: 1rem;
    line-height: 1.3;
}
.news-event-description h3 {
    font-size: 1.1rem;
}
.news-event-description p {
    font-size: .8rem;
}
.btn-read-more {
    text-align: center;
    padding-top: 1.5rem;
}
.btn-read-more a {
    text-decoration: none;
    font-size: 1.5rem;
    font-family: 'Sarabun', sans-serif;
    padding: 3px 2rem;
    border: 3px solid #095845;
    color: #095845;
    transition: .3s;
}
.btn-read-more a:hover {
    background: #095845;
    color: #fff;
}
@media only screen and (max-width: 500px){
    .news-event-topic h1 {
        font-size: 1.8rem;

    }
}


footer#footer-top {
    background: #333333;
    width: 100%;
    padding: 2rem;
}
.footer-list-menu {
    display: flex;
    align-items: center;
    justify-content: space-between;
}
.footer-menu {
    display: flex;
    flex-wrap: wrap;
}
.footer-menu li {
    list-style: none;
    padding: 0 10px;
}
.footer-menu li a {
    text-decoration: none;
    color: #666;
    font-weight: 700;
}
.footer-info-wrapper {
    display: flex;
    justify-content: space-between;
    align-items: center;
}
.info-l {
    margin: 1rem 0;
}
.info-address p{
    color: #808080;
    font-family: 'Sarabun', sans-serif;
    font-size: .8rem;
}
.info-social {
    display: flex;
    margin: .5rem 0;
}
.info-social img {
    width: 28px;
    color: #666;
    stroke: #666666;
    fill: #666666;
    padding: 0 4px;
}

.info-r {
    display: flex;
}
.info-product-select {
    display: flex;
    align-items: center;
    border: 3px solid #c9c9c9;
    padding: 10px 15px;
    margin-right: 1rem;
}
.info-product-select p, .info-product-select i {
    font-size: 1.2rem;
    color: #c9c9c9;
    font-weight: 700;
    padding: 0 5px;
}
.info-contact {
    line-height: 1.8;
}
.info-contact li {
    list-style: none;
    color: #808080;
}

footer#footer-bottom {
    background: #4d4d4d;
    height: 45px;
    line-height: 45px;
    text-align: center;
}
.footer-copyright p {
    color: #808080;
}

@media only screen and (max-width: 1000px){
    .footer-info-wrapper {
        flex-direction: column;
        align-items: flex-start;
    }
}
@media only screen and (max-width: 600px) {
    .info-r {
        flex-direction: column;
    }
    .info-contact {
        padding: 1rem 0;
    }
    .footer-copyright p {
        font-size: .8rem;
    }
}
